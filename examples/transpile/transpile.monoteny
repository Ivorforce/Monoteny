-- import(monoteny.dialects.common.0.0);

@main
def main() :: {
    print("Test");
}

def main_py() :: {
    print("Test Python");
}

@transpile
def transpile(using: transpiler 'Transpiler) :: {
    if transpiler.language != #.Python {
        transpiler.transpile(main);
    }
    else {
        -- For whatever reason, use some other main function for python.
        transpiler.transpile(main_py, as_main: true);
    }
}
