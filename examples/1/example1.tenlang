// alias: floor_div
pattern {} /. {} :: MultiplicationPrecedence;

fun {lhs: $Number} /. {rhs: $Number} -> $Number :: lhs / rhs;

fun square(_ a: $Number) -> $Number :: a * a;
fun {self: $Number}.square() -> $Number :: self * self;

fun main() :: {
    var b = 5;
    b = 3;

    print("b: ");
    print(5 > 2 ** 3 /. 2);
    print(5 > 2 || true);
    print(2 ** 3 ** 5 + 2);
    print([b, square(b), 2.square()]);
}
