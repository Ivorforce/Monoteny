// alias: floor_div
pattern {} /. {} :: MultiplicationPrecedence;

fun {lhs: $Float} /. {rhs: $Float} -> $Float :: lhs / rhs;

fun square(_ a: $Number) -> $Number :: a * a;
fun {self: $Number}.square() -> $Number :: self * self;

fun main() :: {
    let a = square(2) * 2;

    var b = 5.1;
    b = b.square() ** 2.2 /. 2.4;

    print("Test: ");
    print(a);
    print(b);
}
