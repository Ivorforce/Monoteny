-- alias: floor_div
pattern(MultiplicationPrecedence) :: {} /_ {};

fun {lhs: $Float} /_ {rhs: $Float} -> $Float :: lhs / rhs;

fun square(_ a: $Number) -> $Number :: a * a;
fun {self: $Number}.square() -> $Number :: square(self);

fun main() :: {
    print("Test: ");

    let a: Float32 = 2.2.square() * 3;

    var b = square(5);
    b = b.square() ** -2.2 /_ a // 2;

    print(b < 2 || a > 2);
}
